{
  "manifest_version": 3,
  "name": "GW Checker",
  "description": "GwChecker description",
  "version": "3.1",
  "content_scripts": [
    {
      "matches": [
        "https://*.gwars.io/*",
        "https://gwars.io/*",
        "https://www.gwars.io/*",
        "https://*.gwars.io/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "bundle.js"
      ]
    }
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "gwChecker"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}